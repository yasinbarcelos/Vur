# API Configuration
VITE_API_URL=http://localhost:8080/api/v1
VITE_WS_URL=ws://localhost:8080/ws

# App Configuration
VITE_APP_NAME=VUR - Time Series Platform
VITE_APP_VERSION=1.0.0

# Environment
VITE_NODE_ENV=development

